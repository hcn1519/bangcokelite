                          <% else %>
                              <span>|</span>
                              <% unless c.meal1.nil? %>
                                  <span><%= c.meal1 %>,</span>
                              <% end %>
                              <% unless c.meal2.nil? %>
                                  <span><%= c.meal2 %>,</span>
                              <% end %>
                              <% unless c.meal3.nil? %>
                                  <span><%= c.meal3 %></span>
                              <% end %>
                          
                          <% end %>
                        </div>
