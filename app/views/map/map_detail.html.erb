                            <img src="/images/icons/wifi2.png" style="margin-right:2px;">
                          <% else %>
                            <img src="/images/icons/wifi2.png" style="margin-right:2px;">
                          <% end %>
                          <% if c.personal_option8 =="화장실" %>
