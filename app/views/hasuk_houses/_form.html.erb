$(document).ready(function(){
  
  $(".ForValueZero").click(function(){
     var zero = 0;

    <% for i in 1..2 %>
      var distance_value<%= i %> = $(".distance<%= i %>").val();
      var distance_time_value<%= i %> = $(".distance_time<%= i %>").val();
    <% end %>
    
    <% for i in 1..2 %>
      if (distance_value<%= i %> == "") {
        $(".distance<%= i %>").val(zero);
      }
      if (distance_time_value<%= i %> == ""){
        $(".distance_time<%= i %>").val(zero);
      }
    <% end %>
    
    <% for i in 1..2 %>
      var trans_distance<%= i %>_value = $(".trans_distance<%= i %>").val();
      var trans_distance_time<%= i %>_value = $(".trans_distance_time<%= i %>").val();
    
      if (trans_distance<%= i %>_value == "") { $(".trans_distance<%= i %>").val(zero); }
      if (trans_distance_time<%= i %>_value == "") { $(".trans_distance_time<%= i %>").val(zero); }
    <% end %>
   
  });
