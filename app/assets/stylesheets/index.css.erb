main {
  margin-top:0px;
}
.col-sm-12 {
    text-align: center;
}
.navbar-default {
    background:none;
    color:#e5e9ea;
}
#univ_btn {
  font-family:source-han-sans-korean; 
  border-radius:8px; 
  background: #ffffff; 
  color: #999999; 
  border: 2px solid #999999;
}
#section_btn {
  font-family:source-han-sans-korean; 
  border-radius:8px; 
  background: #ffffff;
  color: #999999; 
  border: 2px solid #999999;
}
#submit_btn {
  border: 2px solid #2ecc71; 
  border-radius: 8px; 
  margin-left: 5px; 
  font-family:source-han-sans-korean;
}
footer {
    background-color: #2c3341;
    padding: 0;
    padding-top: 10px;
}

#darkblue-box {
    background-color: #2c3341;
}

#dark-box {
    background-color: #000000;
}

.white-text {
    color: #ffffff;
}

.btn-info {
    background: none;
    border-style: solid;
    border-width: 1px;
    border-color: #ff8773;
    color: #ff8773;
    font-size: 16px;
}

.btn-info:hover,
.btn-info.hover,
.btn-info:focus,
.btn-info:active,
.btn-info.active,
.open > .dropdown-toggle.btn-info {
  color: #ffffff;
  background-color: #ff8773;
  border-color: #ff8773;
}

.pink {
    color: #ff8773;
}

.darkblue-text {
    color: #2c3341;
}


#info_button {
    background: url(<%= asset_data_uri 'background.jpg' %>);
}

.point2 {
    color: #2ecc71; 
}

p {
    display: block;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;
}


.col-sm-7, .col-lg-7, .col-md-7 > .feature-title {
    margin-bottom: 0;    
}

.btn-group > .btn, .btn-group > .submit, .authform .btn-group > .button, .btn-group > .button-xs, .btn-group-vertical > .btn, .btn-group-vertical > .submit, .authform .btn-group-vertical > .button, .btn-group-vertical > .button-xs {
    position: relative;
    float: none;
}
body {
    background-color: #ffffff;
    color: #666666;
}
.row {
    margin: 0;
}
.content-section {
    padding-top: 75px;
    padding-bottom: 75px;
}
.navbar-default {
    background:#e5e9ea;
    border-bottom:1px solid #BDC3C7;
}
.navbar-default .navbar-toggle:before {
    color:#7F8C8D;
}
.navbar-default .navbar-nav > li > a {
    color:#fff;
}

.navbar-default .navbar-brand {
color:#fff;
}
